{
  "name": "@chan-ko-llc/chan-ko-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' vitest --config ./vitest.config.ts",
    "test:ui": "vitest --ui --config ./vitest.config.ts",
    "coverage": "vitest run --coverage --config ./vitest.config.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc --noEmit",
    "fix": "gts fix",
    "prepare": "pnpm run compile",
    "pretest": "pnpm run compile",
    "posttest": "pnpm run lint"
  },
  "dependencies": {
    "@tsparticles/all": "^3.8.1",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.1.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gts": "^5.3.1",
    "jsdom": "^24.1.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.18",
    "vitest": "^2.1.9"
  }
}
